{
  "name": "fastify-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "prisma generate",
    "build": "rm -rf build && prisma generate > /dev/null && tsc && tsc-alias && rm -rf build/routes/__test__",
    "start": "node build/index.js",
    "dev": "pnpm i && prisma migrate dev && tsx watch src/index.ts",
    "test": "pnpm test:reset && pnpm test:run",
    "test:reset": "export $(cat .env.test | xargs) && prisma migrate reset --force",
    "test:run": "export $(cat .env.test | xargs) && tap --reporter=base --disable-coverage --comments src/**/*.test.ts; exit 0",
    "prisma:add": "prisma migrate dev --name",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reinit": "prisma migrate reset && rm -rf ./prisma/migrations && prisma migrate dev -n init && prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.8.0",
    "@fastify/error": "3.4.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/rate-limit": "9.1.0",
    "@fastify/type-provider-typebox": "4.0.0",
    "@prisma/client": "5.7.1",
    "@sinclair/typebox": "0.32.4",
    "ajv-errors": "3.0.0",
    "dotenv": "16.3.1",
    "env-var": "7.4.1",
    "fast-json-stringify": "5.9.2",
    "fastify": "4.25.2",
    "fastify-common": "1.16.0",
    "fastify-plugin": "4.5.1",
    "prisma": "5.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/tap": "^15.0.11",
    "pino-pretty": "^10.2.3",
    "tap": "^18.6.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}
