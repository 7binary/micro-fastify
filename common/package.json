{
  "name": "fastify-common",
  "version": "1.12.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "yarn build && node ./dist/index.js",
    "build:common": "esbuild src/index.ts --platform=node --outfile=dist/index.js --sourcemap --bundle --minify",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "prepublishOnly": "yarn build:common && yarn build:types",
    "clean": "del ./dist/*",
    "pub": "yarn clean && yarn publish"
  },
  "dependencies": {
    "@fastify/error": "3.3.0",
    "fastify": "4.23.2",
    "fastify-plugin": "4.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "del-cli": "^5.0.0",
    "esbuild": "^0.19.3",
    "typescript": "^4.9.5"
  }
}
