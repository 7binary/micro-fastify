{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node build/index.js",
    "dev": "tsnd -r tsconfig-paths/register --respawn --exit-child --transpile-only --watch src src/index.ts",
    "test": "yarn test:reset && yarn test:run",
    "test:reset": "export $(cat .env.test | xargs) && prisma migrate reset --force",
    "test:run": "export $(cat .env.test | xargs) && tap --ts --reporter=list --no-coverage --comments --node-arg=--require=tsconfig-paths/register src/**/*.test.ts",
    "prisma:migrate": "prisma migrate dev --name",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset && rm -rf ./prisma/migrations && prisma migrate dev --name init && prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/cookie": "8.3.0",
    "@fastify/error": "3.3.0",
    "@fastify/helmet": "11.0.0",
    "@fastify/jwt": "7.2.0",
    "@fastify/type-provider-typebox": "3.3.0",
    "@prisma/client": "5.1.1",
    "@sinclair/typebox": "0.29.1",
    "ajv-errors": "3.0.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "env-var": "7.3.1",
    "fastify": "4.21.0",
    "fastify-common": "1.9.1",
    "fastify-plugin": "4.5.1",
    "prisma": "5.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.14.0",
    "@types/tap": "^15.0.8",
    "pino-pretty": "^9.4.0",
    "tap": "^16.3.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
